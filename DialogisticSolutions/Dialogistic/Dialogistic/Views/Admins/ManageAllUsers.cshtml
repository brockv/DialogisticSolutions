@model IEnumerable<Dialogistic.Models.ChangeUserRoleModel>

@{
    ViewBag.Title = "Manage Callers";
}

<section class="content-header">
    <h1>
        Manage Users
    </h1>
    <ol class="breadcrumb">
        <li><a href="~/Admins/Dashboard"><i class="fa fa-dashboard"></i>Dashboard</a></li>
        <li class="active">Manage Users</li>
    </ol>
</section>
<!-- Main content -->
<section class="the-content">
    <div class="row">
        <div class="col-xs-12">
            <div class="box">
                <div class="box-body">
                    <table class="table table-bordered table-striped">
                        <thead>
                            <tr>
                                <th>User</th>
                                <th>Select new role</th>
                            </tr>
                        </thead>
                        <tbody>
                            @foreach(var item in Model)
                            {

                            <tr>
                                <td>@item.User.FullName</td>
                                <td>@Html.Partial("_ManageAllUsersPartial", item)</td>
                            </tr>
                            }
                        </tbody>
                    </table>
                </div>

            </div>
            <!-- /.box -->
        </div>
        <!-- /.col -->
    </div>
    <!-- /.row -->
</section>
